<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.spark.chipro.manage.biz.mapper.ReviewMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.spark.chipro.manage.biz.entity.Review">
        <id column="REVIEW_ID" property="reviewId" />
        <result column="TASK_ID" property="taskId" />
        <result column="USER_ID" property="userId" />
        <result column="REVIEW_CONTENT" property="reviewContent" />
        <result column="SCOPE" property="scope" />
        <result column="PRODUCTION_ID" property="productionId" />
        <result column="STATUS" property="status" />
        <result column="CREATE_USER" property="createUser" />
        <result column="CREATE_TIME" property="createTime" />
        <result column="UPDATE_USER" property="updateUser" />
        <result column="UPDATE_TIME" property="updateTime" />
        <result column="REMARKS" property="remarks" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        REVIEW_ID AS "reviewId", TASK_ID AS "taskId", USER_ID AS "userId", REVIEW_CONTENT AS "reviewContent", SCOPE AS "scope", PRODUCTION_ID AS "productionId", STATUS AS "status", CREATE_USER AS "createUser", CREATE_TIME AS "createTime", UPDATE_USER AS "updateUser", UPDATE_TIME AS "updateTime", REMARKS AS "remarks"
    </sql>

</mapper>
